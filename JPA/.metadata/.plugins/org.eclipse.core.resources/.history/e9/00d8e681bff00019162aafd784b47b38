package uo.ri.cws.domain;

import alb.util.assertion.Argument;

public class Client {
	
	//PK DNI
	
	private String dni;
	private String name;
	private String surname;
	private String email;
	private String phone;
	private Address address;
	
	public Client(String dni) {
		Argument.isTrue(dni != null);
	}

}

